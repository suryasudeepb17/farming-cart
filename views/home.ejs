	<div class="container mt-3">
        <div class="row">
            <% if(data.length){ %>
                <% for(var i = 0; i< data.length; i++) { %>  
            <div class="product-grid col-xs-12 col-sm-6 col-md-3" id="<%=data[i].id%>">
				<div class="product-item">
				  <div class="image">
					<img src="<%=data[i].image_url%>" alt="Product 1" >
				  </div>
				  <div class="caption">
					<div class="name text-center">
					  <%= data[i].item_name %>
					</div>
					<div class="price">
					  <span>$
                        <%=data[i].cost  %>
						<%=data[i].quantity%>
                      </span>
					</div>
					<div class="cart">
                        <% if(number){ %>
                            <%if(cart_data.includes(data[i].id)) { %>
                                    <a href="/cart" style="background-color:orange" class="btn btn-primary">Item Added! Go to Cart</a>
                            <% } else { %>
                                <form method="post" action="/add-cart">
                                    <input type="hidden" name="number" value="<%=number%>">
                                    <input type="hidden" name="item_code" value="<%=data[i].id%>">
                                    <input type="hidden" name="item_name" value="<%=data[i].item_name%>">
                                    <input type="hidden" name="cost" value="<%=data[i].cost%>">
									<input type="hidden" name="quantity" value="<%=data[i].quantity%>">
                                    <input type="hidden" name="image_url" value="<%=data[i].image_url%>">									
                                    <button type="submit" class="btn btn-primary">Add to Cart</button>
                                </form>
                            <% } %>
                        <%}else{%>
                            <button type="submit" class="btn btn-primary">Login First to add items to cart</button>
                        <% } %>
					</div>
				  </div>
                  <% if(number){ %>
                    <%if(wish_data.includes(data[i].id)) { %>
                        <a href = '/wish'  class="btn btn-default wishlist" data-toggle="tooltip" data-placement="right" title="Wishlist"><i style="color:red" class="fa fa-heart"></i></a>
                        <button type="button" class="btn btn-default compare" data-toggle="tooltip" data-placement="right" title="Compare"><i class="fa fa-circle-o"></i></button> 
                    <% } else { %>
                        <form method="post" action="/add-wish">
                            <input type="hidden" name="number" value="<%=number%>">
                            <input type="hidden" name="item_code" value="<%=data[i].id%>">
                            <input type="hidden" name="item_name" value="<%=data[i].item_name%>">
                            <input type="hidden" name="cost" value="<%=data[i].cost%>">
							<input type="hidden" name="quantity" value="<%=data[i].quantity%>">
                            <input type="hidden" name="image_url" value="<%=data[i].image_url%>">
                            <button type="submit" class="btn btn-default wishlist" data-toggle="tooltip" data-placement="right" title="Wishlist"><i class="fa fa-heart"></i></button>
				            <button type="button" class="btn btn-default compare" data-toggle="tooltip" data-placement="right" title="Compare"><i class="fa fa-circle-o"></i></button>
                        </form>
                    <% } %>
                <%}else{%>
                    <button type="button" class="btn btn-default wishlist" data-toggle="tooltip" data-placement="right" title="Wishlist"><i class="fa fa-heart"></i></button>
				    <button type="button" class="btn btn-default compare" data-toggle="tooltip" data-placement="right" title="Compare"><i class="fa fa-circle-o"></i></button>
                <% } %>
				</div>
			</div>
